        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.webhope.emergency.dao.UserTestDao">
    
    <select id="queryByName" parameterType="string" resultType="com.webhope.emergency.entity.UserTest">
